|BEGIN:VCALENDAR
|VERSION:2.0
|PRODID:-//Rory King//Hull-iCal v1.0//EN
|CALSCALE:GREGORIAN
|METHOD:PUBLISH
|X-WR-CALNAME:!{user.name.substr(0, user.name.indexOf(" "))}'s Hull University Calendar
|X-WR-TIMEZONE:Europe/London
|BEGIN:VTIMEZONE
|TZID:Europe/London
|X-LIC-LOCATION:Europe/London
|BEGIN:DAYLIGHT
|TZOFFSETFROM:+0000
|TZOFFSETTO:+0100
|TZNAME:BST
|DTSTART:19700329T010000
|RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
|END:DAYLIGHT
|BEGIN:STANDARD
|TZOFFSETFROM:+0100
|TZOFFSETTO:+0000
|TZNAME:GMT
|DTSTART:19701025T020000
|RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
|END:STANDARD
|END:VTIMEZONE

each event in events
	|!{null}
	|BEGIN:VEVENT
	|UID:!{event.uid}@hull.ac.uk
	|DTSTART;TZID=Europe/London:!{event.start}
	|DTEND;TZID=Europe/London:!{event.end}
	|SUMMARY:!{event.title}
	|DESCRIPTION:!{event.teachers}
	|LOCATION:!{event.location}
	|END:VEVENT

|!{null}
|END:VCALENDAR